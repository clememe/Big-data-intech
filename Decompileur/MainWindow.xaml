<Window x:Class="Decompileur.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Decompileur"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="550">
    <Grid>
        <Button Content="Start" HorizontalAlignment="Left" Margin="314,109,0,0" VerticalAlignment="Top" Width="80" Click="StartClick"/>
        <Button Content="Close" HorizontalAlignment="Left" Margin="410,109,0,0" VerticalAlignment="Top" Width="80" Click="CloseClick"/>
        <TextBox x:Name="CSVPath" HorizontalAlignment="Left" Margin="10,33,0,0" TextWrapping="Wrap" Text="CSV file location" VerticalAlignment="Top" Width="384" IsReadOnly="True"/>
        <Button Content="Browse" HorizontalAlignment="Left" Margin="410,32,0,0" VerticalAlignment="Top" Width="80" Click="BrowseClick"/>
        <Label Content="Number of lines :" HorizontalAlignment="Left" Margin="111,106,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="LinesNumber" HorizontalAlignment="Left" Margin="223,110,0,0" TextWrapping="Wrap" Text="100000" VerticalAlignment="Top" Width="62" RenderTransformOrigin="0.621,0.528"/>

    </Grid>
</Window>
